{
  "name": "hello-world-avs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start:operator": "ts-node operator/index.ts",
    "start:traffic": "ts-node operator/createNewTasks.ts",
    "start:anvil": "anvil",
    "contracts:clean": "cd contracts && forge clean",
    "deploy:core": "npm run build && cd contracts && forge script script/DeployEigenLayerCore.s.sol --rpc-url http://localhost:8545 --broadcast",
    "deploy:hello-world": "npm run build && cd contracts && forge script script/HelloWorldDeployer.s.sol --rpc-url http://localhost:8545 --broadcast",
    "deploy:core-debug": "npm run deploy:core -- --revert-strings debug",
    "deploy:hello-world-debug": "npm run deploy:hello-world -- --revert-strings debug",
    "build": "cd contracts && forge build",
    "extract:abis": "npm run build && node utils/abis.js",
    "test": "jest ./tests",
    "lint": "eslint . --ignore-pattern 'contracts/**'",
    "prettier": "prettier --check \"**/*.{js,ts,json,md}\" \"!contracts/**\"",
    "prettier:fix": "prettier --write \"**/*.{js,ts,json,md}\" \"!contracts/**\""
  },
  "dependencies": {
    "commander": "^12.1.0",
    "dotenv": "^10.0.0",
    "ethers": "^6.13.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.5",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
